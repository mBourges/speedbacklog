  },
  "homepage": "https://github.com/mBourges/speedbacklog#readme",
  "dependencies": {
    "auth0-lock": "^9.0.3",
    "axios": "^0.11.0",
    "boom": "^3.1.2",
    "good": "^6.6.0",
    "good-console": "^5.3.1",
    "hapi": "^13.3.0",
